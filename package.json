{
  "name": "fintech-mobile-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.8.8",
    "@react-navigation/native": "^7.1.22",
    "@react-navigation/native-stack": "^7.8.2",
    "@react-navigation/stack": "^7.5.0",
    "@supabase/supabase-js": "^2.84.0",
    "expo": "~54.0.18",
    "expo-camera": "~17.0.10",
    "expo-image-picker": "~17.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "lucide-react-native": "^0.546.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.14.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "babel-jest": "^30.2.0",
    "eslint": "^9.14.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.4",
    "prettier": "^3.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup-after-env.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",

      "^react-native-vector-icons/(.*)$": "<rootDir>/__mocks__/react-native-vector-icons/$1.js",
      "^react-native-vector-icons$": "<rootDir>/__mocks__/react-native-vector-icons.js",

      "^@expo/vector-icons$": "<rootDir>/__mocks__/expoVectorIconsMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|expo|expo-image-picker|expo-modules-core|react-native-reanimated|react-native-gesture-handler)/)"
    ]
  }
}
